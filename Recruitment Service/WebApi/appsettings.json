{
  "RabbitMQ": {
    "HostName": "localhost",
    "UserName": "",
    "Password": "",
    "VirtualHostName": "/"
  },
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog",
      "Serilog.Exceptions",
      "Serilog.Sinks.Network"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/serilog.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "TCPSink",
        "Args": {
          "uri": "tcp://logstash:5000",
          "textFormatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "RMS"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "gRPC": {
    "Address": ""
  },
  "Auth0": {
    "Domain": "",
    "Audience": "",
    "Connection": "",
    "ClientId": "",
    "ClientSecret": "",
    "ManagementClientId" : "",
    "ManagementClientSecret" : ""
  },
  "BlobStorage": {
    "Endpoint": "",
    "AccessKey": "",
    "SecretKey": "",
    "BucketName": ""
  },
  "AllowedHosts": "*"
}
